        <property>
            <name>timing_configuration_type</name>
        </property>
    </system_timing_properties>
    
    <!-- Defines the properties of the entire system -->
    <system_properties>
        <property>
            <name>system_parameter</name>
            <type>int</type>
            <!-- Supported types must be defined !-->
            <value>42</value>
        </property>
    </system_properties>
    
    <!-- Defines the properties of one particualar participant -->
    <element_instances_properties>
        <element_instance>
            <!-- Unique name in the system -->
            <id>FrontSensor</id>
            <properties>
                <property>
                    <name>sensor_position/X</name>
                    <type>float</type>
                    <value>1.234</value>
                </property>
                <property>
                    <name>sensor_position/Y</name>
                    <type>float</type>
                    <value>0.00</value>
                </property>
                <property>
                    <name>sensor_position/Z</name>
                    <type>float</type>
                    <value>5.00</value>
                </property>
                <property>
                    <name>parameter1</name>
                    <type>string</type>
                    <value>value1</value>
                </property>
            </properties>
        </element_instance>
    
        <element_instance>
